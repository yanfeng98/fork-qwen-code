{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["ES2021", "DOM", "DOM.Iterable"],
    "isolatedModules": true,
    "skipLibCheck": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src"]
}
